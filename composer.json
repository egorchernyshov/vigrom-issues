{
    "name": "egorchernyshov/vigrom-issues",
    "description": "Vigrom issues",
    "license": "MIT",
    "prefer-stable": true,
    "require": {
        "php": "^7.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.1",
        "fzaninotto/faker": "^1.9"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/App"
        },
        "classmap": [
            "third_party_library/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit --coverage-text"
    }
}
